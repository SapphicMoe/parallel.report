{
  "name": "@solely/parallel",
  "version": "3.0.0",
  "type": "module",
  "author": "Chloe Arciniega <solely@riseup.net>",
  "license": "zlib",
  "private": true,
  "scripts": {
    "astro:check": "astro check",
    "build": "yarn build:check && astro build",
    "build:check": "yarn astro:check && tsc --noEmit",
    "dev": "yarn start:dev",
    "format:prettier": "prettier --w --plugin-search-dir=. --cache .",
    "lint:eslint": "eslint --fix src/**/*.{ts,astro}",
    "start": "astro preview",
    "start:dev": "astro dev",
    "test": "eslint . --ext .ts,.js,.astro && prettier --c .",
    "wrangler:dev": "wrangler pages dev --proxy 3000 -- yarn dev",
    "wrangler:start": "wrangler pages dev --proxy 3000 -- yarn start"
  },
  "repository": "https://github.com/solelychloe/parallel.report",
  "packageManager": "yarn@3.6.0",
  "dependencies": {
    "astro": "^3.4.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.2.1",
    "@astrojs/mdx": "^1.1.3",
    "@astrojs/prefetch": "^0.4.1",
    "@astrojs/tailwind": "^5.0.2",
    "@catppuccin/tailwindcss": "^0.1.6",
    "@fontsource/atkinson-hyperlegible": "^5.0.17",
    "@fontsource/inter": "^5.0.15",
    "@tailwindcss/typography": "^0.5.10",
    "@types/eslint": "^8.44.6",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "astro-compress": "^2.0.14",
    "astro-eslint-parser": "^0.16.0",
    "astro-iconify": "^1.2.0",
    "astro-seo": "^0.8.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.1",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "rehype-external-links": "^3.0.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "wrangler": "^3.15.0"
  }
}
